{
  "name": "fishivo-monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@4.9.2+sha512.1fc009bc09d13cfd0e19efa44cbfc2b9cf6ca61482725eb35bbc5e257e093ebf4130db6dfe15d604ff4b79efd8e1e8e99b25fa7d0a6197c9f9826358d4d65c3c",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "NODE_ENV=production turbo run build",
    "start": "turbo run start --parallel",
    "mobile": "turbo run start --filter=@fishivo/native",
    "web": "turbo run dev --filter=@fishivo/web",
    "android": "turbo run android --filter=@fishivo/native",
    "android:build": "turbo run android:build --filter=@fishivo/native",
    "apk": "turbo run apk --filter=@fishivo/native",
    "aab": "turbo run aab --filter=@fishivo/native",
    "android:install": "adb install -r apps/native/android/app/build/outputs/apk/debug/app-debug.apk",
    "android:install:release": "adb install -r apps/native/android/app/build/outputs/apk/release/app-release.apk",
    "android:clean": "turbo run android:clean --filter=@fishivo/native",
    "android:start": "adb shell am start -n com.fishivo/.MainActivity",
    "android:reload": "curl -X POST http://localhost:8081/reload",
    "android:menu": "adb shell input keyevent 82",
    "android:restart": "adb shell am force-stop com.fishivo && adb shell am start -n com.fishivo/.MainActivity",
    "ios": "turbo run ios --filter=@fishivo/native",
    "ios:build": "turbo run ios:build --filter=@fishivo/native",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "clean": "turbo run clean && rm -rf node_modules",
    "lint": "turbo run lint",
    "format": "turbo run //#format",
    "format:fix": "turbo run //#format:fix",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-arrow-functions": "^7.22.5",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.24.7",
    "@babel/plugin-transform-optional-chaining": "^7.24.8",
    "@babel/plugin-transform-runtime": "^7.26.0",
    "@babel/plugin-transform-shorthand-properties": "^7.22.5",
    "@babel/plugin-transform-template-literals": "^7.22.5",
    "@babel/preset-env": "^7.25.3",
    "@react-native-community/cli": "18.0.0",
    "@react-native/babel-preset": "0.78.2",
    "@react-native/eslint-config": "0.78.2",
    "@react-native/metro-config": "0.78.2",
    "@react-native/typescript-config": "^0.80.1",
    "@rnx-kit/align-deps": "^3.0.8",
    "@types/jest": "29.5.13",
    "@types/node": "20.11.5",
    "@types/pg": "^8",
    "@types/react": "^19.0.7",
    "@types/react-native-vector-icons": "6.4.14",
    "@types/ws": "^8",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "babel-jest": "29.7.0",
    "babel-plugin-module-resolver": "5.0.0",
    "eslint": "8.56.0",
    "eslint-plugin-jest": "^29.0.1",
    "jest": "^29.6.3",
    "patch-package": "7.0.0",
    "prettier": "3.0.3",
    "react": "19.0.0",
    "react-native": "0.78.2",
    "react-native-clean-project": "4.0.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-test-renderer": "^19.0.0",
    "tsup": "^8.5.0",
    "turbo": "^2.5.4",
    "typescript": "5.3.3"
  },
  "rnx-kit": {
    "alignDeps": false
  },
  "dependencies": {
    "@gorhom/portal": "^1.0.14",
    "@hookform/resolvers": "^5.1.1",
    "@modelcontextprotocol/sdk": "^1.15.1",
    "@radix-ui/react-switch": "^1.2.5",
    "@rnmapbox/maps": "^10.1.43",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.55.0",
    "@types/nodemailer": "^6.4.17",
    "commander": "^13.1.0",
    "date-fns": "^4.1.0",
    "debounce": "^2.2.0",
    "nanoid": "^5.1.5",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.5",
    "pg": "^8.16.3",
    "react-hook-form": "^7.59.0",
    "react-native-image-crop-picker": "^0.51.0",
    "zod": "^3.25.71",
    "zod-to-json-schema": "^3.24.6"
  },
  "resolutions": {
    "react-native-reanimated": "^3.8.1"
  }
}
