{
  "name": "@fishivo/api",
  "version": "1.0.0",
  "description": "API services for Fishivo",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "react-native": "./dist/index.native.js",
      "default": "./dist/index.web.js"
    },
    "./client/supabase.native": {
      "types": "./dist/client/supabase.native.d.ts",
      "default": "./dist/client/supabase.native.js"
    },
    "./client/supabase.web": {
      "types": "./dist/client/supabase.web.d.ts",
      "default": "./dist/client/supabase.web.js"
    },
    "./client/supabase.server": {
      "types": "./dist/client/supabase.server.d.ts",
      "default": "./dist/client/supabase.server.js"
    },
    "./services/posts": {
      "types": "./dist/services/posts/index.d.ts",
      "react-native": "./dist/services/posts/index.js",
      "default": "./dist/services/posts/index.js"
    },
    "./services/posts/posts.native": {
      "types": "./dist/services/posts/posts.native.d.ts",
      "default": "./dist/services/posts/posts.native.js"
    },
    "./services/native": {
      "types": "./dist/services/native.d.ts",
      "default": "./dist/services/native.js"
    },
    "./services/image": {
      "types": "./dist/services/image/index.d.ts",
      "default": "./dist/services/image/index.js"
    },
    "./services/image/image.native": {
      "types": "./dist/services/image/image.native.d.ts",
      "default": "./dist/services/image/image.native.js"
    },
    "./services/image/image.web": {
      "types": "./dist/services/image/image.web.d.ts",
      "default": "./dist/services/image/image.web.js"
    },
    "./services/comments": {
      "types": "./dist/services/comments/index.d.ts",
      "default": "./dist/services/comments/index.js"
    },
    "./services/comments/comment-likes.native": {
      "types": "./dist/services/comments/comment-likes.native.d.ts",
      "default": "./dist/services/comments/comment-likes.native.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@fishivo/types": "workspace:*",
    "@fishivo/utils": "workspace:*",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.3",
    "nodemailer": "^6.10.1",
    "zod": "^4.0.13"
  },
  "optionalDependencies": {
    "@react-native-async-storage/async-storage": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/nodemailer": "^6.4.17",
    "typescript": "^5.3.3"
  }
}
